<?php

$(macro) {
    $(
        // "as alias" due to https://github.com/marcioAlmada/yay/issues/56
        returnType() as alias
    )
} >> {
    [
        $(alias ... {
            'return',
            $(returnTypeNullable ? {
                'nullable',
            })
            $$(stringify($(returnTypeName)))
        })
    ]
}

return [
    : string,
    : ?string
];
