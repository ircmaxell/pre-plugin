<?php

$(macro) {
    $(
        // "as alias" due to https://github.com/marcioAlmada/yay/issues/56
        visibilityModifiers() as alias
    )
} >> {
    [
        $(alias ...(,) {
            $$(stringify($(visibilityModifier)))
        }) ,
    ]
}

return [
    public,
    protected,
    private,
    static,
    public static,
];
