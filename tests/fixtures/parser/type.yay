<?php

$(macro) {
    $(
        // "as alias" due to https://github.com/marcioAlmada/yay/issues/56
        type() as alias
    )
} >> {
    [
        $$(stringify($(alias)))
    ]
}

return [
    string,
    int,
    integer,
    bool,
    boolean,
    float,
    mixed,
    array,
    callable,
    resource,
    stdClass,
    \stdClass,
    \Foo\Bar\Baz,
];
